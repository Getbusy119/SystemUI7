<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android" 
    android:ordering="together">
    <objectAnimator android:propertyName="OverlayAlpha" 
        android:duration="1000"
        android:valueFrom="255"
        android:valueTo="0"
        android:interpolator="@android:anim/linear_interpolator"
        android:valueType="intType"/>
    <objectAnimator android:propertyName="HoleScale" 
        android:duration="1200"
        android:valueFrom="1.0"
        android:valueTo="0.0"
        android:interpolator="@android:anim/decelerate_interpolator"
        android:valueType="floatType"/>
	<objectAnimator android:propertyName="BgOneTranslate" 
        android:duration="1200"
        android:valueFrom="@dimen/leather_color_one_translate"
        android:valueTo="0.0"
        android:interpolator="@android:anim/accelerate_decelerate_interpolator"
        android:valueType="floatType"/>
	<objectAnimator android:propertyName="BgTwoTranslate" 
        android:duration="1200"
        android:startOffset="100"
        android:valueFrom="@dimen/leather_color_two_translate_start"
        android:valueTo="@dimen/leather_color_two_translate_end"
        android:interpolator="@android:anim/accelerate_decelerate_interpolator"
        android:valueType="floatType"/>
	<objectAnimator android:propertyName="BgThreeTranslate" 
        android:duration="1200"
        android:startOffset="300"
        android:valueFrom="@dimen/leather_color_one_translate"
        android:valueTo="0.0"
        android:interpolator="@android:anim/accelerate_decelerate_interpolator"
        android:valueType="floatType"/>
	<objectAnimator android:propertyName="BgFourTranslate" 
        android:duration="920"
        android:startOffset="600"
        android:valueFrom="@dimen/leather_color_three_translate"
        android:valueTo="0.0"
        android:interpolator="@android:anim/accelerate_decelerate_interpolator"
        android:valueType="floatType"/>
	<set android:ordering="together">
	    <objectAnimator android:propertyName="FirstLineTextScale"
		    android:duration="300"
		    android:startOffset="1160"
		    android:valueFrom="0.9"
		    android:valueTo="1.0"
		    android:interpolator="@android:anim/linear_interpolator"
		    android:valueType="floatType" />
	    <objectAnimator android:propertyName="FirstLineTextAlpha"
		    android:duration="300"
		    android:valueFrom="0"
		    android:valueTo="255"
		    android:interpolator="@android:anim/linear_interpolator"
		    android:valueType="intType" />
	</set>
	<set android:ordering="together">
	    <objectAnimator android:propertyName="SecondLineTextScale"
		    android:duration="300"
		    android:startOffset="1240"
		    android:valueFrom="0.9"
		    android:valueTo="1.0"
		    android:interpolator="@android:anim/linear_interpolator"
		    android:valueType="floatType" />
       <objectAnimator android:propertyName="SecondLineTextAlpha"
	    android:duration="300"
	    android:valueFrom="0"
	    android:valueTo="255"
	    android:interpolator="@android:anim/linear_interpolator"
	    android:valueType="intType" />
	</set>
	
</set>
