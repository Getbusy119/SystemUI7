<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:ordering="together">
    <set android:ordering="together">
        <objectAnimator
	        android:propertyName="FirstCircleAlpha"  
	        android:duration="400"
		    android:valueFrom="0"  
		    android:valueTo="255"  
		    android:interpolator="@android:anim/linear_interpolator"
		    android:valueType="intType" />
       <objectAnimator
	        android:propertyName="SecondCircleScale"  
	        android:duration="400"
		    android:valueFrom="0.0f"  
		    android:valueTo="1.0f"  
		    android:interpolator="@android:anim/linear_interpolator"
		    android:valueType="floatType" />
    </set>
    <set android:ordering="sequentially">
        <objectAnimator
	        android:propertyName="ThreeCircleAlpha"  
	        android:duration="600"
	        android:startOffset="400"
		    android:valueFrom="0"  
		    android:valueTo="38"  
		    android:interpolator="@android:anim/linear_interpolator"
		    android:valueType="intType" />
    	<objectAnimator
	        android:propertyName="ThreeCircleStrokeWidth"  
	        android:duration="300"
		    android:valueFrom="1.0"  
		    android:valueTo="0.0"  
		    android:interpolator="@android:anim/linear_interpolator"
		    android:valueType="floatType" />
    </set>
    <set android:ordering="sequentially">
        <objectAnimator 
            android:propertyName="SweepAngle" 
            android:duration="600"
            android:startOffset="400"
            android:valueFrom="0.0"
            android:valueTo="353.0"
            android:interpolator="@android:anim/decelerate_interpolator"
            android:valueType="floatType"/>
        <objectAnimator 
            android:propertyName="SweepStrokeWidthScale" 
            android:duration="300"
            android:valueFrom="1.0"
            android:valueTo="0.0"
            android:interpolator="@android:anim/linear_interpolator"
            android:valueType="floatType"/>
    </set>
    <set android:ordering="together">
        <set android:ordering="together">
            <objectAnimator 
	            android:propertyName="alphaOne" 
	            android:duration="150"
	            android:startOffset="600"
	            android:valueFrom="0"
	            android:valueTo="255"
	            android:interpolator="@android:anim/linear_interpolator"
	            android:valueType="intType"/>
            <objectAnimator 
	            android:propertyName="translateOne" 
	            android:duration="150"
	            android:valueFrom="0.0"
	            android:valueTo="1.0"
	            android:interpolator="@android:anim/linear_interpolator"
	            android:valueType="floatType"/>
        </set>
        <set android:ordering="together">
            <objectAnimator 
	            android:propertyName="alphaTwo" 
	            android:duration="150"
	            android:startOffset="650"
	            android:valueFrom="0"
	            android:valueTo="255"
	            android:interpolator="@android:anim/linear_interpolator"
	            android:valueType="intType"/>
            <objectAnimator 
	            android:propertyName="translateTwo" 
	            android:duration="150"
	            android:valueFrom="0.0"
	            android:valueTo="1.0"
	            android:interpolator="@android:anim/linear_interpolator"
	            android:valueType="floatType"/>
        </set>
        <set android:ordering="together">
            <objectAnimator 
	            android:propertyName="alphaThree" 
	            android:duration="150"
	            android:startOffset="700"
	            android:valueFrom="0"
	            android:valueTo="255"
	            android:interpolator="@android:anim/linear_interpolator"
	            android:valueType="intType"/>
            <objectAnimator 
	            android:propertyName="translateThree" 
	            android:duration="150"
	            android:valueFrom="0.0"
	            android:valueTo="1.0"
	            android:interpolator="@android:anim/linear_interpolator"
	            android:valueType="floatType"/>
        </set>
        <set android:ordering="together">
            <objectAnimator 
	            android:propertyName="alphaFour" 
	            android:duration="150"
	            android:startOffset="750"
	            android:valueFrom="0"
	            android:valueTo="255"
	            android:interpolator="@android:anim/linear_interpolator"
	            android:valueType="intType"/>
            <objectAnimator 
	            android:propertyName="translateFour" 
	            android:duration="150"
	            android:valueFrom="0.0"
	            android:valueTo="1.0"
	            android:interpolator="@android:anim/linear_interpolator"
	            android:valueType="floatType"/>
        </set>
        <set android:ordering="together">
            <objectAnimator 
	            android:propertyName="alphaFive" 
	            android:duration="150"
	            android:startOffset="800"
	            android:valueFrom="0"
	            android:valueTo="255"
	            android:interpolator="@android:anim/linear_interpolator"
	            android:valueType="intType"/>
            <objectAnimator 
	            android:propertyName="translateFive" 
	            android:duration="150"
	            android:valueFrom="0.0"
	            android:valueTo="1.0"
	            android:interpolator="@android:anim/linear_interpolator"
	            android:valueType="floatType"/>
        </set>
    </set>
    <set android:ordering="sequentially">
        <objectAnimator 
            android:propertyName="AlphaWeekAndDay" 
            android:duration="300"
            android:startOffset="900"
            android:valueFrom="0"
            android:valueTo="255"
            android:interpolator="@android:anim/linear_interpolator"
            android:valueType="intType"/>
        
        <objectAnimator 
            android:propertyName="PhoneAndSmsScale" 
            android:duration="100"
            android:valueFrom="0.0"
            android:valueTo="1.0"
            android:interpolator="@android:anim/linear_interpolator"
            android:valueType="floatType"/>
        
        <objectAnimator 
            android:propertyName="PhoneAndSmsNumberScale" 
            android:duration="100"
            android:valueFrom="0.0"
            android:valueTo="1.0"
            android:interpolator="@android:anim/linear_interpolator"
            android:valueType="floatType"/>
                
        <set android:ordering="together">
            <objectAnimator 
	            android:propertyName="UpTranslate" 
	            android:duration="1000"
	            android:valueFrom="1"
	            android:valueTo="60"
	            android:repeatMode="restart"
	            android:repeatCount="2"
	            android:interpolator="@android:anim/accelerate_decelerate_interpolator"
	            android:valueType="intType"/>
            <objectAnimator 
	            android:propertyName="UpAlpha" 
	            android:duration="1000"
	            android:valueFrom="0.0"
	            android:valueTo="1.0"
	            android:repeatMode="restart"
	            android:repeatCount="2"
	            android:interpolator="@android:anim/accelerate_decelerate_interpolator"
	            android:valueType="floatType"/>
        </set>
    </set>
</set>
