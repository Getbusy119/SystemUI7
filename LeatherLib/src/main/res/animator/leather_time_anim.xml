<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:ordering="together">
    <set android:ordering="together">
        <objectAnimator
	        android:propertyName="BgScale"  
	        android:duration="400"
		    android:valueFrom="0.8"  
		    android:valueTo="1.0"  
		    android:interpolator="@android:anim/accelerate_decelerate_interpolator"
		    android:valueType="floatType"  />
        <objectAnimator
	        android:propertyName="BgAlpha"  
	        android:duration="400"
		    android:valueFrom="0"  
		    android:valueTo="255"  
		    android:interpolator="@android:anim/linear_interpolator"
		    android:valueType="intType"  />
    </set>
    
    <objectAnimator
        android:propertyName="TrayScale"  
        android:duration="400"
        android:startOffset="100"
	    android:valueFrom="0.0"  
	    android:valueTo="1.0"  
	    android:valueType="floatType"  />
    
    <set android:ordering="together" >
        <objectAnimator
	        android:propertyName="HourAlpha"  
	        android:duration="800"
	        android:startOffset="400"
		    android:valueFrom="0"  
		    android:valueTo="255"  
		    android:valueType="intType"  />
        <objectAnimator
	        android:propertyName="HourRotate" 
	        android:duration="800" 
		    android:valueFrom="0.0"  
		    android:valueTo="90.0"  
		    android:interpolator="@android:anim/decelerate_interpolator"
		    android:valueType="floatType"  />
    </set>
    
    <set android:ordering="together" >
        <objectAnimator
	        android:propertyName="MinuteAlpha"  
	        android:duration="800"
	        android:startOffset="500"
		    android:valueFrom="0"  
		    android:valueTo="255"  
		    android:valueType="intType"  />
        <objectAnimator
	        android:propertyName="MinuteRotate" 
	        android:duration="800" 
		    android:valueFrom="0.0"  
		    android:valueTo="90.0"  
		    android:interpolator="@android:anim/decelerate_interpolator"
		    android:valueType="floatType"  />
    </set>
    
    <set android:ordering="together" >
        <objectAnimator
	        android:propertyName="SecondAlpha"  
	        android:duration="800"
	        android:startOffset="600"
		    android:valueFrom="0"  
		    android:valueTo="255"  
		    android:valueType="intType"  />
        <objectAnimator
	        android:propertyName="SecondRotate" 
	        android:duration="800" 
		    android:valueFrom="0.0"  
		    android:valueTo="90.0"  
		    android:interpolator="@android:anim/decelerate_interpolator"
		    android:valueType="floatType"  />
    </set>
    
	<set android:ordering="sequentially" >
        <objectAnimator
	        android:propertyName="LightTwoAlpha"  
	        android:duration="540"
	        android:startOffset="400"
		    android:valueFrom="0"  
		    android:valueTo="255"  
		    android:valueType="intType"  />
        <objectAnimator
	        android:propertyName="LightTwoAlpha" 
	        android:duration="900" 
	        android:startOffset="300"
		    android:valueFrom="255"  
		    android:valueTo="0"  
		    android:valueType="intType"  />
        <set android:ordering="together">
		    <objectAnimator
			    android:propertyName="SecondRun"  
			    android:duration="1000"
			    android:startOffset="10"
				android:valueFrom="1"  
				android:valueTo="1000"  
				android:repeatMode="restart"
				android:repeatCount="infinite"
				android:valueType="floatType"
				android:interpolator="@android:anim/linear_interpolator"  />
		    <set android:ordering="sequentially">
		        <objectAnimator 
		            android:propertyName="PhoneAndSmsScale" 
		            android:duration="100"
		            android:valueFrom="0.0"
		            android:valueTo="1.0"
		            android:interpolator="@android:anim/linear_interpolator"
		            android:valueType="floatType"/>
		        
		        <objectAnimator 
		            android:propertyName="PhoneAndSmsNumberScale" 
		            android:duration="100"
		            android:valueFrom="0.0"
		            android:valueTo="1.0"
		            android:interpolator="@android:anim/linear_interpolator"
		            android:valueType="floatType"/>
		                
		        <set android:ordering="together">
		            <objectAnimator 
			            android:propertyName="UpTranslate" 
			            android:duration="1000"
			            android:valueFrom="1"
			            android:valueTo="60"
			            android:repeatMode="restart"
			            android:repeatCount="2"
			            android:interpolator="@android:anim/accelerate_decelerate_interpolator"
			            android:valueType="intType"/>
		            <objectAnimator 
			            android:propertyName="UpAlpha" 
			            android:duration="1000"
			            android:valueFrom="0.0"
			            android:valueTo="1.0"
			            android:repeatMode="restart"
			            android:repeatCount="2"
			            android:interpolator="@android:anim/accelerate_decelerate_interpolator"
			            android:valueType="floatType"/>
		        </set>
		    </set>
		</set>
    </set>
    <set android:ordering="sequentially" >
        <objectAnimator
	        android:propertyName="LightThreeAlpha"  
	        android:duration="540"
	        android:startOffset="700"
		    android:valueFrom="0"  
		    android:valueTo="255"
		    android:interpolator="@android:anim/linear_interpolator"  
		    android:valueType="intType"  />
        <objectAnimator
	        android:propertyName="LightThreeAlpha" 
	        android:duration="400" 
	        android:startOffset="200"
		    android:valueFrom="255"  
		    android:valueTo="0"  
		    android:valueType="intType"  />
    </set>
    <objectAnimator
        android:propertyName="WeekAlpha"  
        android:duration="300"
        android:startOffset="900"
	    android:valueFrom="0"  
	    android:valueTo="255"  
	    android:interpolator="@android:anim/linear_interpolator"
	    android:valueType="intType"  />
    
    <objectAnimator
        android:propertyName="DayOfWeekRotate"  
        android:duration="200"
        android:startOffset="980"
	    android:valueFrom="0.0"  
	    android:valueTo="30.0"  
	    android:interpolator="@android:anim/accelerate_decelerate_interpolator"
	    android:valueType="floatType"  />
</set>
